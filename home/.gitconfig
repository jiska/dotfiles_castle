[include]
	path = ~/.gitconfig.local
[color]
	ui = auto
[alias]
	b = branch
	bd = branch -d
	can = commit --amend --no-edit
	cb = checkout -b
	cd = checkout develop
	ci = commit --verbose
	cm = checkout master
	co = checkout
	co- = checkout -
	coo = checkout --ours
	cot = checkout --theirs
	cp = cherry-pick -x
	d = diff --patience
	ds = diff --staged --patience
	g = grep
	gi = grep -I
	grc = grep --count
	grl = grep --files-with-matches
	l = log --graph --pretty='format:%C(yellow)%h%Cblue%d%Creset %s %C(black bold)%an, %ar%Creset'
	m = merge
	n = now --all --stat
	p = pull
	sd = svn dcommit --rmdir --interactive
	sf = svn fetch
	sr = svn rebase
	st = status --short --branch
[core]
	excludesfile = ~/.gitignore.global
	fscache = true
	pager = less -r
	precomposeunicode = true
	preloadindex = true
	quotepath = false
	whitespace = trailing-space,space-before-tab
	commentChar = ";"
[pager]
	status = true
[push]
	default = simple
[merge]
	ff = false
[pull]
	rebase = true
[fetch]
	prune = true
[hub]
	protocol = https
[diff "xlsx"]
	binary = true
	textconv = $GOPATH/bin/git-xlsx-textconv
[diff "exif"]
	textconv = exiftool
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
